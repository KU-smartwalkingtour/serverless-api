{
  "name": "ku-swt",
  "version": "1.0.0",
  "description": "KU Smart Walking Tour Serverless API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "sst dev --stage dev",
    "deploy": "sst deploy --stage production",
    "deploy:dev": "sst deploy --stage dev",
    "destroy": "sst remove --stage production",
    "destroy:dev": "sst remove --stage dev",
    "build": "node build.js",
    "clean": "rm -rf dist .sst",
    "docker:build": "docker build --no-cache -t cloudwebservice-server:optimized .",
    "format": "prettier --write .",
    "postinstall": "ln -sf ../src/utils node_modules/utils && ln -sf ../src/services node_modules/services && ln -sf ../src/config node_modules/config && ln -sf ../src/models node_modules/models"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.974.0",
    "@aws-sdk/client-dynamodb": "^3.940.0",
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/client-sesv2": "^3.917.0",
    "@aws-sdk/lib-dynamodb": "^3.940.0",
    "@tmcw/togeojson": "^7.1.2",
    "@xmldom/xmldom": "^0.8.11",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "gpx-parse": "^0.10.4",
    "iconv-lite": "^0.7.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^10.1.0",
    "proj4": "^2.19.10",
    "sequelize": "^6.37.7",
    "serverless-http": "^4.0.0",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "xml2js": "^0.6.2",
    "yauzl": "^3.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "esbuild": "^0.24.0",
    "javascript-obfuscator": "^4.1.1",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.6.2",
    "sst": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "_moduleAliases": {
    "@models": "./models",
    "@routes": "./routes",
    "@utils": "./utils",
    "@middleware": "./middleware",
    "@config": "./config"
  }
}
